on property:sys.boot_completed=1
    write /sys/block/zram0/comp_algorithm lzo-rle
    write /proc/sys/vm/page-cluster 0
    write /proc/sys/vm/swappiness 190
    write /sys/kernel/mm/swap/vma_ra_enabled false
    swapon_all ${MAGISKTMP}/fstab.genom
    setprop ro.zram.mark_idle_delay_mins 60
    setprop ro.zram.first_wb_delay_mins 180
    setprop ro.zram.periodic_wb_delay_hours 24
